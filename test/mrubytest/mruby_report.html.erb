<html>
<head>
  <meta charset='utf-8' />
  <title>mruby/c mrubytest report</title>
</head>
<body>

  <% $tests.each do |test| %>
    <h2><%=h test['category'] %></h2>
    <% test['cases'].each do |t| %>
      <div>
        <h3><%=h t['title'] %></h3>
        <pre style='float: left; width: 50%'><%=h t['rb_txt'] %></pre>
        <pre style='float: left; width: 50%'><%=h t['out'] %></pre>
        <br style='clear:both'>
      </div>
    <% end %>
  <% end %>

</body>
</html>

